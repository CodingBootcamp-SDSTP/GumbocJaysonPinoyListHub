<?xml version="1.0" encoding="UTF-8" ?>
<web-app>
	<display-name>PinoyListHub</display-name>
	<description>Your Phlippine Real Estate Connection</description>
	<servlet>
		<servlet-name>PropertyListings</servlet-name>
		<servlet-class>PropertyListingsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PropertyListings</servlet-name>
		<url-pattern>/listings</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>PropertySpecialist</servlet-name>
		<servlet-class>PropertySpecialistServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PropertySpecialist</servlet-name>
		<url-pattern>/specialist</url-pattern>
	</servlet-mapping>
</web-app>


